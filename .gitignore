!**.gitignore

.gradle/
.idea/
build/
*/build/
!gradle-wrapper.jar

!*.jar
!**/*.jar

!*.java
!**/*.java

!*.aar
!**/*.aar

!*.xml
!**/*.xml

!*.gradle
!**/*.gradle